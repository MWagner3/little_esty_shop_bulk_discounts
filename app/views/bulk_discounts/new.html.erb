<h1>Create New Bulk Discount</h1>
<%= form_with url: "/merchant/#{@merchant.id}/bulk_discounts", method: :post, local: true do |f| %>
  <%= f.label :percentage_discount, "Percentage Discount" %>
  <%= f.text_field :percentage_discount, required: true  %><br/><br/>
  <%= f.label :quantity_threshold, "Quantity Threshold" %>
  <%= f.text_field :quantity_threshold, required: true  %><br/><br/>
  <%= f.submit 'Save'%>
<% end %>
